{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)\\.html$", "dest": "/$1", "status": 308 },
    { "src": "/index.html", "dest": "/", "status": 308 },
    { "src": "/admin.html", "dest": "/admin", "status": 308 },
    { "src": "/home.html", "dest": "/", "status": 308 },
    { "src": "/Blog.html", "dest": "/blog", "status": 308 },
    { "src": "/quotebook.html", "dest": "/quotebook", "status": 308 },
    { "src": "/other.html", "dest": "/other", "status": 308 },
    { "src": "/basement.html", "dest": "/basement", "status": 308 },
    { "src": "/about.html", "dest": "/about", "status": 308 },
    { "src": "/sitemap.html", "dest": "/sitemap", "status": 308 }
  ]
}
